@ARTICLE{Clark2019-is,
  title = "Representation and relative abundance of cell-type selective markers in whole-kidney {RNA-Seq} data",
  author = "Clark, Jevin Z and Chen, Lihe and Chou, Chung-Lin and Jung, Hyun Jun and Lee, Jae Wook and Knepper, Mark A",
  abstract = "Bulk-tissue RNA-Seq is increasingly being used in the study of
