@ARTICLE{Segerstolpe2016-oe,
  title    = "{Single-Cell} Transcriptome Profiling of Human Pancreatic Islets
              in Health and Type 2 Diabetes",
  author   = "Segerstolpe, {\AA}sa and Palasantza, Athanasia and Eliasson,
